{
  "name": "mmir-plugin-speech-nuance-lang",
  "version": "1.0.1",
  "description": "tools for querying supported languages (ASR and TTS) and voices (TTS) by Nuance SpeechKit",
  "main": "./index.js",
  "types": "index.d.ts",
  "directories": {
    "lib": "./www"
  },
  "scripts": {
    "build": "npm run install-res && npm run build-ts && npm run build-dist && npm run build-metadata",
    "install-res": "node res/install.js",
    "build-ts": "tsc",
    "build-dist": "webpack",
    "build-metadata": "createplugincompat ./ && pluginexport ./"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mmig/mmir-plugin-speech-nuance-lang.git"
  },
  "keywords": [
    "speech",
    "language",
    "nuance",
    "speechkit"
  ],
  "author": "russa",
  "license": "MIT",
  "devDependencies": {
    "mmir-lib": "^6.1.0",
    "mmir-plugin-exports": "^1.5.0",
    "mmir-plugin-lang-support": "^1.3.2",
    "ts-loader": "^6.2.2",
    "typescript": "^3.9.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
